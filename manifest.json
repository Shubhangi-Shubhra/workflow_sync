{
    "manifest_version": 3,
    "name": "Workflow Sync",
    "version": "1.0",
    "description": "An intuitive task management application designed to streamline your productivity.",
    "permissions": [
      "history",
      "storage",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
  